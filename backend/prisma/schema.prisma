generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
 id            Int     @id @default(autoincrement())
  email         String  @unique
  name          String
  password      String
  surname       String
  documentId    String  @unique
  birthday      DateTime
  phoneNumber   String
  address       String
  city          String
  cp            String
  iban          String
  occupationTarget  String?
  employeePosition  String?
}
